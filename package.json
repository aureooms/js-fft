{
  "name": "@aureooms/js-fft",
  "description": "Fast Fourier Transform algorithm for JavaScript",
  "version": "2.0.4",
  "author": "Aur√©lien Ooms <aurelien.ooms@gmail.com>",
  "ava": {
    "require": [
      "@babel/polyfill",
      "@babel/register"
    ],
    "files": [
      "test/src/**/*"
    ],
    "timeout": "1m"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "env": {
      "development": {
        "sourceMaps": "inline"
      }
    }
  },
  "bugs": {
    "url": "https://github.com/aureooms/js-fft/issues"
  },
  "dependencies": {},
  "devDependencies": {
    "@aureooms/js-algorithms": "3.0.7",
    "@aureooms/js-array": "4.0.0",
    "@aureooms/js-complex": "1.2.0",
    "@aureooms/js-number": "3.1.0",
    "@aureooms/js-random": "2.0.0",
    "@babel/cli": "7.10.1",
    "@babel/core": "7.10.2",
    "@babel/polyfill": "7.10.1",
    "@babel/preset-env": "7.10.2",
    "@babel/register": "7.10.1",
    "ava": "3.9.0",
    "coveralls": "3.1.0",
    "esdoc": "1.1.0",
    "esdoc-inject-script-plugin": "1.0.0",
    "esdoc-inject-style-plugin": "1.0.0",
    "esdoc-standard-plugin": "1.0.0",
    "np": "6.2.4",
    "nyc": "15.1.0"
  },
  "files": [
    "lib"
  ],
  "homepage": "https://aureooms.github.io/js-fft",
  "keywords": [
    "Fast Fourier Transform",
    "Fourier",
    "fast",
    "fft",
    "ifft",
    "inverse",
    "javascript",
    "js",
    "transform"
  ],
  "license": "AGPL-3.0",
  "main": "lib/index.js",
  "repository": {
    "url": "https://github.com/aureooms/js-fft.git",
    "type": "git"
  },
  "scripts": {
    "build": "rm -rf lib && babel src -d lib",
    "cover": "nyc --reporter=lcov npm test",
    "esdoc": "esdoc",
    "prepare": "npm run build",
    "release": "np",
    "test": "ava"
  }
}
